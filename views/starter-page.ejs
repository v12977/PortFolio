<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <title>Setting-page</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  
  <!-- Favicons -->
  <link href="/img/logoicon.png" rel="icon">
  <link href="/img/logoicon.png" rel="apple-touch-icon">

  <!-- Fonts -->
  <link href="https://fonts.googleapis.com" rel="preconnect">
  <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/vendor/aos/aos.css" rel="stylesheet">
  <link href="/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Main CSS File -->
  <link href="/css/main.css" rel="stylesheet">
  <link href="/css/edit.css" rel="stylesheet">

</head>

<body class="starter-page-page">

  <header id="header" class="header dark-background d-flex flex-column">
    <i class="header-toggle d-xl-none bi bi-list"></i>

    <div class="profile-img">
      <img src="<%=bd.primage%>" alt="" class="img-fluid rounded-circle">
    </div>

    <a href="<%=url%>/" class="logo d-flex align-items-center justify-content-center">
      <!-- Uncomment the line below if you also wish to use an image logo -->
      <!-- <img src="/img/logo.png" alt=""> -->
      <h1 class="sitename"><%=bd.name%></h1>
    </a>

    <div class="social-links text-center">
<% for(let i of ssw){%>
  <% if(i.type=='website'){%>
    <% if(i.name=='github'||i.name=='linkedin'||i.name=='linkedin '||i.name=='github '){%>
      <a href="<%=i.bio_url%>" class="<%=i.name%>"><i class="bi bi-<%=i.name%>"></i></a>
    <%}else{%>
      <a href="<%=i.bio_url%>" class="<%=i.name%>"><i class="bi bi-link-45deg"></i></a>
    <%}%>
  <%}%>
<%}%>
    </div>

    <nav id="navmenu" class="navmenu">
      <ul>
        <li><a href="#hero"><i class="bi bi-house navicon"></i>Home</a></li>
        <li><a href="#about"><i class="bi bi-person navicon"></i> About</a></li>
        <li><a href="#skills"><i class="bi bi-file-earmark-code navicon"></i>Skills</a></li>
        <li><a href="#resume"><i class="bi bi-file-earmark-text navicon"></i> Resume</a></li>
        <li><a href="#portfolio"><i class="bi bi-images navicon"></i> Portfolio</a></li>
        <li><a href="#services"><i class="bi bi-hdd-stack navicon"></i> Services</a></li>
        <!-- dropdown -->
<!-- ..........................................dropdown ...................................... -->
        <!-- dropdown -->
        <li><a href="<%=url%>/logout"><i class="bi bi-envelope navicon"></i> LogOut</a></li>
      </ul>
    </nav>

  </header>

  <main class="main">


        <!-- Page Title -->
    <div class="page-title dark-background">
      <div class="container d-lg-flex justify-content-between align-items-center">
        <h1 class="mb-2 mb-lg-0">Setting Page</h1>
        <nav class="breadcrumbs">
          <ol id="servicesedit">
            <li><a href="<%=url%>/">PortFolio</a></li>
            <li><a type="button" >Edit</a></li>
          </ol>
        </nav>
      </div>
    </div>
    <!-- End Page Title -->


<!-- ..................................Home Data Edit section.......................................... -->
    <!-- Hero Section -->
    <section class="hero section dark-background" id="hero">

      <img src="<%=bd.bgimage%>" alt="" data-aos="fade-in" class="">

      <div class="container" data-aos="fade-up" data-aos-delay="100">
        <!-- ..........update name and show profession form...... -->
        <form class="dataform" action="<%=url%>/update/basic/data" method="post">
          <label for="username">Name : </label>
          <input type="text" placeholder="Enter Name" value="<%=bd.name%>" id="username" name="name" required>

          <label for="userprof">Profession : </label>
          <input type="text" placeholder="Enter Your Profession with seprated by ," value="<%=bd.showprofession%>" id="userprof" name="showprofession" required>

          <div class="btn"><button type="submit">Update</button></div>
        </form>
        <!-- ......form ended......-->


        <form class="dataform" action="<%=url%>/upload/basicdata/pr" method="post" enctype="multipart/form-data">

          <label for="profileimg">Profile-img : </label>
          <input type="file" id="profileimg" name="profilePic" required>

          <div class="btn"><button type="submit">Update</button></div>

        </form>
        <form class="dataform" action="<%=url%>/upload/basicdata/bg" method="post" enctype="multipart/form-data">

          <label for="bgimg">Background-img : </label>
          <input type="file" id="bgimg" name="profilePic" required>

          <div class="btn"><button type="submit">Update</button></div>

        </form>
<!-- ...............form for add website.................... -->
        <form class="dataform" action="<%=url%>/add/ssw/website" method="post">
          <label for="websitename">Website Name : </label>
          <input type="text" placeholder="name in smallercase eg: github,linkedin" id="websitename" name="name" required>

          <label for="websitelink">Website Link : </label>
          <input type="text" placeholder="Enter Your Website link" id="websitelink" name="bio_url" required>

          <div class="btn"><button type="submit">Submit</button></div>
        </form>
<!-- ...................form ended............. -->
      </div>

    </section><!-- /Hero Section -->

<!-- ........................................edit about section data ................................. -->
    <!-- About Section -->
    <section id="about" class="about section">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        <h2>About</h2>
      </div><!-- End Section Title -->

      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4 justify-content-center">
          <div class="col-lg-8 content">
<!--.............................form for the summary data ...................................  -->
            <form class="dataform" action="<%=url%>/update/basic/data" method="post">
              
              <div class="col-lg-6 dataformcon">
                <label for="aboutsummary">Abt-Summary&nbsp;:</label>
                <textarea rows="5" cols="140" id="aboutsummary" name="aboutsummary" required><%=bd.aboutsummary%></textarea>
              </div>
              <div class="col-lg-6 dataformcon">
                <label for="skillsummary">skl-Summary&nbsp;:</label>
                <textarea rows="5" cols="140" id="skillsummary" name="skillsummary" required><%=bd.skillsummary%></textarea>
              </div>
              <div class="col-lg-6 dataformcon">
                <label for="finalsummary">fil-Summary&nbsp;:</label>
                <textarea rows="5" cols="140" id="finalsummary" name="finalsummary" required><%=bd.finalsummary%></textarea>
              </div>

              <div class="btn"><button type="submit">Update</button></div>
            </form>

            <!-- ..................form ended................ -->
<!-- .......................................form for other data ................................ -->
            <form class="dataform" style="margin-top: 3rem;" action="<%=url%>/update/basic/data" method="post">
              
              <div class="col-lg-6">
                <ul>
                  <li><i class="bi bi-chevron-right"></i> <label for="birthday">Birthday&nbsp;: <input type="date" id="birthday" value="<%=bd.birthdate%>" name="birthdate" required style="width:100%"></label></li>
                  <li><i class="bi bi-chevron-right"></i> <label for="website">Website&nbsp;:<input type="text" id="website" name="website" value="<%=bd.website%>" required style="width:100%"></label></li>
                  <li><i class="bi bi-chevron-right"></i> <label for="phone">Phone &nbsp; &nbsp;:<input type="text" id="phone" name="phone" value="<%=bd.phone%>" required style="width:100%"></label></li>
                  <li><i class="bi bi-chevron-right"></i> <label for="city">City &nbsp; &nbsp; &nbsp; &nbsp;:<input type="text" id="city" name="city" value="<%=bd.city%>" required style="width:100%"></label></li>
                </ul>
              </div>
              <div class="col-lg-6">
                <ul>
                  <li><i class="bi bi-chevron-right"></i> <label for="bstskill">Bst-Skill : <input type="text" name="bstskill" value="<%=bd.bstskill%>" id="bstskill" required style="width:100%"></label></li>
                  <li><i class="bi bi-chevron-right"></i> <label for="degree">Degree &nbsp; : <input type="text" id="degree" name="degree" value="<%=bd.degree%>" required style="width:100%"></label></li>
                  <li><i class="bi bi-chevron-right"></i> <label for="email">Email &nbsp; &nbsp; : <input type="email" id="email" name="email" value="<%=bd.email%>" required style="width:100%"></label></li>
                  <li><i class="bi bi-chevron-right"></i> <label for="working">Working : <input type="text" id="working" name="workingstatus" value="<%=bd.workingstatus%>" required style="width:100%"></label></li>
                </ul>
              </div>
              <div class="btn"><button type="submit">Submit</button></div>
            </form>
        <!-- form ended -->
          </div>
        </div>

      </div>

    </section><!-- /About Section -->

        <!-- Stats Section -->
    <section id="stats" class="stats section">

      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4">

          <div class="col-lg-3 col-md-6">
            <div class="stats-item">
<!-- .........................form for online projects............ -->
              <form class="dataform" action="<%=url%>/update/basic/data" method="post">
                <label for="onlineproject">Online Projects</label>
                <input type="text" placeholder="0" value="<%=bd.onlineproject%>" id="onlineproject" name="onlineproject" required>
                <div class="btns"><button type="submit">Submit</button></div>
              </form>
<!-- form ended -->
            </div>
          </div><!-- End Stats Item -->

          <div class="col-lg-3 col-md-6">
            <div class="stats-item">
<!-- .........................form for total projects............ -->
              <form class="dataform" action="<%=url%>/update/basic/data" method="post">
                <label for="totalproject">Total Projects</label>
                <input type="number" placeholder="0" value="<%=bd.totalproject%>" name="totalproject" id="totalproject" required>
                <div class="btns"><button type="submit">Submit</button></div>
              </form>
<!-- form ended -->
            </div>
          </div><!-- End Stats Item -->

          <div class="col-lg-3 col-md-6">
            <div class="stats-item">
<!-- .........................form for single projects............ -->
              <form class="dataform" action="<%=url%>/update/basic/data" method="post">
                <label for="singleproject">Single Projects</label>
                <input type="number" placeholder="0" value="<%=bd.singleproject%>" id="singleproject" name="singleproject" required>
                <div class="btns"><button type="submit">Submit</button></div>
              </form>
<!-- form ended -->
            </div>
          </div><!-- End Stats Item -->

          <div class="col-lg-3 col-md-6">
            <div class="stats-item">
<!-- .........................form for online projects............ -->
              <form class="dataform" action="<%=url%>/update/basic/data" method="post">
                <label for="groupproject">Group Projects</label>
                <input type="number" id="groupproject" value="<%=bd.groupproject%>" placeholder="0" name="groupproject" required>
                <div class="btns"><button type="submit">Submit</button></div>
              </form>
<!-- form ended-->
            </div>
          </div><!-- End Stats Item -->

        </div>

      </div>

    </section><!-- /Stats Section -->


        <!-- Skills Section -->
    <section id="skills" class="skills section light-background">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        <h2>Skills</h2>
<!-- ......................add skill form  -->
        <form class="dataform" action="<%=url%>/add/ssw/skill" method="post">
          <label for="newskill">Add Skill:</label>
          <input type="text" id="newskill" name="name" required>
          <label for="percentage">Percentage:</label>
          <input type="number" id="percentage" name="bio_url" required>
          <div class="btn"><button type="submit">Submit</button></div>
        </form>
<!-- form ended -->
      </div><!-- End Section Title -->

      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row skills-content skills-animation" id="allskills">

          <div class="conform" style="display: none;" id="skillconformdelete">
            <p>Are You Sure? Delete SkillName</p>
            <a class="con" href="#"><button type="submit">Yes</button> </a>
            <a class="con" href="#"><button type="cancel">No</button></a>
          </div>

          <div class="col-lg-6">
<%for(let s of skill){%>
  <%if(i<=c){%>
            <div class="progress">
              <span class="skill"><span><%=s.name%></span> <i class="val"><%=s.bio_url%>%  <a class="trush"><i class="bi bi-trash3"></i></a>
                  <id style="display: none;"><%=s.id%></id></i></span>
              <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="<%=s.bio_url%>" aria-valuemin="0" aria-valuemax="100"></div>
              </div>
            </div>
  <%}%>
  <%i+=1%>
<%}%>
<%i=1%>
            <!-- End Skills Item -->

          </div>

          <div class="col-lg-6">
<%for(let s of skill){%>
  <%if(c<i){%>
            <div class="progress">
              <span class="skill"><span><%=s.name%></span> <i class="val"><%=s.bio_url%>%  <a class="trush"><i class="bi bi-trash3"></i></a>
                  <id style="display: none;"><%=s.id%></i></span>
              <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="<%=s.bio_url%>" aria-valuemin="0" aria-valuemax="100"></div>
              </div>
            </div>
  <%}%>
  <%i+=1%>
<%}%>
            <!-- End Skills Item -->

          </div>

        </div>

      </div>

    </section><!-- /Skills Section -->

        <!-- Resume Section -->
    <section id="resume" class="resume section">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        <h2>Resume</h2>
<!-- ................................form for the resume................ -->
         <form class="dataform" action="<%=url%>/add/resume/data" method="post">

          <label for="type">Type : </label>
          <select id="type" name="type">
            <option value="Education">Education</option>
            <option value="Professional">Professional Experience</option>
          </select>

          <label for="rname">Name : </label>
          <input type="text" id="rname" name="rname" required>

          <label for="year">Year : </label>
          <input type="text" id="year" name="year" placeholder="Formate= Frome-To" required>

          <label for="university">University/Schoole</label>
          <input type="text" id="university" name="university" required>

          <label for="rbio">bio</label>
          <textarea cols="68" id="rbio" name="rbio" required></textarea>

          <div class="btn"><button type="submit">Submit</button></div>
         </form>
         <!-- ended resume form -->
      </div>
      <!-- End Section Title -->

      <div class="container">

        <div class="row re-content" id="allresume">

          <div class="conform" style="display: none;" id="resumeconformdelete">
            <p>Are You Sure? Delete SkillName</p>
            <a class="con" href="#"><button type="submit">Yes</button> </a>
            <a class="con"><button type="cancel">No</button></a>
          </div>

          <div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">
            <h3 class="resume-title">Education</h3>
<% for(r of rd){%>
  <%if(r.rtype=='Education'){%>
            <div class="resume-item">
              <h4><%=r.rname%></h4>
              <h5><%=r.ryear%></h5>
              <p><em><%=r.runiversity%></em></p>
              <p><%=r.rbio%></p>
              <a class="trush"><i class="bi bi-trash3"></i></a>
              <id style="display: none;"><%=r.id%></id>
            </div>
  <%}%>
<%}%>
            <!-- Edn Resume Item -->

          </div>

          <div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
            <h3 class="resume-title">Professional Experience</h3>
<% for(r of rd){%>
  <%if(r.rtype=='Professional'){%>
            <div class="resume-item">
              <h4><%=r.rname%></h4>
              <h5><%=r.ryear%></h5>
              <p><em><%=r.runiversity%></em></p>
              <p><%=r.rbio%></p>
              <a class="trush"><i class="bi bi-trash3"></i></a>
              <id style="display: none;"><%=r.id%></id>
            </div>
  <%}%>
<%}%>
            <!-- Edn Resume Item -->

          </div>

        </div>

      </div>

    </section><!-- /Resume Section -->

        <!-- Portfolio Section -->
    <section id="portfolio" class="portfolio section light-background">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        
        <h2>Portfolio & Achievements</h2>

<!-- ........................form for the portfolio................................. -->
        <form class="dataform" action="<%=url%>/add/portfolio/data" method="post">
          <label for="pname">Project Name : </label>
          <input type="text" id="pname" name="name" required>
          <label for="type">Type : </label>
          <select id="type" name="type">
            <option name="type" value="Project">Project</option>
            <option name="type" value="Achievement">Achievement</option>
          </select>
          <div class="btn"><button type="submit">Submit</button></div>
        </form>
<!-- form ended -->
      </div><!-- End Section Title -->

      <div class="container">

        <div class="isotope-layout" data-default-filter="*" data-layout="masonry" data-sort="original-order">

          <ul class="portfolio-filters isotope-filters" data-aos="fade-up" data-aos-delay="100">
            <li data-filter="*" class="filter-active">All</li>
            <li data-filter=".filter-app">Project</li>
            <li data-filter=".filter-product">Achievement</li>
            <!-- <li data-filter=".filter-branding">Branding</li> -->
            <!-- <li data-filter=".filter-books">Books</li> -->
          </ul><!-- End Portfolio Filters -->

          <div class="row gy-4 isotope-container re-content" data-aos="fade-up" data-aos-delay="200" id="allportfolio">

            <div class="conform" style="height: 80%; display:none;" id="portfolioconformdelete">
              <p>Are You Sure? Delete SkillName</p>
              <a class="con"><button type="submit">Yes</button> </a>
              <a class="con"><button type="cancel">No</button></a>
            </div>
<%for(p of pfd){%>
  <% if(p.type=='Project'){%>
            <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-app">
              <div class="portfolio-content h-100">
                <a class="trush deleter "><i class="bi bi-trash3"></i></a>
                <id style="display: none;"><%=p.id%></id>
                <img src="<%=p.projectimage%>" class="img-fluid" alt="">
                <div class="portfolio-info">
                  <h4>Project</h4>
                  <p><%=p.name%></p>
                  <a href="<%=p.projectimage%>" title="Project" data-gallery="portfolio-gallery-app" class="glightbox preview-link"><i class="bi bi-zoom-in"></i></a>
                  <a href="<%=url%>/setting/portfolio/<%=p.id%>" title="More Details" class="details-link"><i class="bi bi-link-45deg"></i></a>
                </div>
              </div>
            </div>
  <%}%>
<%}%>
<%for(p of pfd){%>
  <% if(p.type!='Project'){%>
            <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-product">
              <div class="portfolio-content h-100">
                <a class="trush deleter "><i class="bi bi-trash3"></i></a>
                <id style="display: none;"><%=p.id%></id>
                <img src="<%=p.projectimage%>" class="img-fluid" alt="">
                <div class="portfolio-info">
                  <h4>Project</h4>
                  <p><%=p.name%></p>
                  <a href="<%=p.projectimage%>" title="Project" data-gallery="portfolio-gallery-app" class="glightbox preview-link"><i class="bi bi-zoom-in"></i></a>
                  <a href="<%=url%>/setting/portfolio/<%=p.id%>" title="More Details" class="details-link"><i class="bi bi-link-45deg"></i></a>
                </div>
              </div>
            </div>
  <%}%>
<%}%>
            <!-- End Portfolio Item -->


          </div><!-- End Portfolio Container -->

        </div>

      </div>

    </section><!-- /Portfolio Section -->


    <!-- Services Section -->
    <section id="services" class="services section">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        <h2>Services</h2>
        <form class="dataform" action="<%=url%>/add/ssw/service" method="post">
          <label for="servicename">Service Name : </label>
          <input type="text" id="servicename" placeholder="Enter Service Name" name="name" required>
          <label for="servicebio">Service Bio : </label>
          <input type="text" id="servicebio" placeholder="Enter Your Service Bio" name="bio_url" required>

          <div class="btn"><button type="submit">Submit</button></div>
        </form>
      </div><!-- End Section Title -->

      <div class="container">

        <div class="row gy-4 re-content" id="allservices">

          <div class="conform" style="height: 150%; display:none;" id="serviceconformdelete">
            <p>Are You Sure? Delete SkillName</p>
            <a class="con" href="#"><button type="submit">Yes</button> </a>
            <a class="con" href="#"><button type="cancel">No</button></a>
          </div>
<% for(s of ssw){%>
  <%if(s.type=='service'){%>
          <div class="col-lg-4 col-md-6 service-item d-flex" data-aos="fade-up" data-aos-delay="100">
            <div class="icon flex-shrink-0"><i class="bi bi-briefcase"></i></div>
            <div>
              <h4 class="title"><a class="stretched-link"><%=s.name%></a></h4>
              <a class="trush deleter " style="margin: -1.5rem 1rem 0rem 0rem;"><i class="bi bi-trash3"></i></a>
              <id style="display: none;"><%=s.id%></id>
              <p class="description"><%=s.bio_url%></p>
            </div>
          </div>
  <%}%>
<%}%>
          <!-- End Service Item -->

        </div>

      </div>

    </section><!-- /Services Section -->

    

  </main>

