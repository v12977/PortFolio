            <a class="con" id="servicecencel"><button type="cancel">No</button></a>
          `;
          serviceconformdelete.style.display='flex';
          let servicecencel=document.getElementById('servicecencel');
          servicecencel.addEventListener('click',()=>{
            serviceconformdelete.style.display='none';
          });
        });
